{
    "name": "oilshell-extension",
    "displayName": "OilShell extension",
    "description": "Extension for Oil Shell Script language.",
    "publisher": "karino2",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/karino2/vscode-oilshell"
    },
    "version": "1.2.0",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "oilshell",
                "aliases": [
                    "Oil Shell Script",
                    "oilshell"
                ],
                "extensions": [
                    ".oil"
                ],
                "firstLine": "^#!/.*\\boil\\b",
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "oilshell",
                "scopeName": "source.shell.oilshell",
                "path": "./syntaxes/oilshell.tmLanguage.json"
            }
        ]
    }
}
